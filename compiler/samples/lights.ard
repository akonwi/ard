use ard/io

enum Color {
  Red,
  Yellow,
  Green
}

fn get_meaning(light: Color) Str {
  match light {
    // map a pattern to a simple expression
    Color::Red => "Stop",
    // or a block
    Color::Yellow => {
      "Yield"
    },
    // use _ for catch alls
    _ => "Go"
  }
}

fn main() {
  io::print("Yellow means {get_meaning(Color::Yellow)}")
}
