enum Color {
  Red,
  Yellow,
  Green
}

fn get_meaning(light: Color) Str {
  match light {
    Color::Red => "Stop",
    Color::Yellow => "Yield",
    Color::Green => "Go"
  }
}

print("The signal is ${get_meaning(Color::Red)}")
print("The signal is ${get_meaning(Color::Green)}")
print("The signal is ${get_meaning(Color::Yellow)}")
