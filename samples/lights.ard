use std/io

enum Color {
  Red,
  Yellow,
  Green
}

fn get_meaning(light: Color) Str {
  match light {
    Color::Red => "Stop",
    Color::Yellow => {
      // the value can also be returned from a block
      "Yield"
    },
    Color::Green => "Go"
  }
}

io.print("Yellow means {{get_meaning(Color::Yellow)}}")
